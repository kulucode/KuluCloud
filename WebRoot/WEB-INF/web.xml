<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<listener>
		<listener-class>tt.kulu.bi.base.BSLoginListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>TTService</servlet-name>
		<servlet-class>com.tt4j2ee.c.TTService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BSJSEngin</servlet-name>
		<servlet-class>com.tt4j2ee.c.BSJSEngin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TTService</servlet-name>
		<url-pattern>/TTService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BSJSEngin</servlet-name>
		<url-pattern>/ttjs/*</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>20</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
